{
  "input_folder": "images",
  "output_file": "timelapse_hq.mp4",
  "fps": 30,
  "width": 1920,
  "height": 1080,
  "codec": "h264",
  "quality": 98,
  "fade_in_frames": 60,
  "fade_out_frames": 60,
  "hold_frames": 30,
  "interpolation_recursions": 6,
  "enable_interpolation": true,
  "enable_alignment": true,
  "alignment_method": "ecc",
  "alignment_reference_mode": "most_zoomed",
  "alignment_scale": 0.2,
  "enable_fade_effects": true,
  "enable_stabilization": true,
  "crop_to_stable": false,
  "batch_size": 1,
  "preview_mode": false,
  "preview_frames": 100,
  "supported_formats": [".jpg", ".jpeg", ".png", ".bmp", ".tiff"]
}
